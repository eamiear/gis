{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./documents/doc/gois-api"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "./documents/generator/templates/jsdoc3Template",

    "systemName"            : "Gosuncn Gis Api",
    "footer"                : "",
    "copyright"             : "gosuncn@0.0.1",
    "includeDate"           : true,
    "navType"               : "inline",
    "theme"                 : "cerulean",
    "linenums"              : true,
    "dateFormat"            : "YYYY-MM-DD, h:mm:ss a"
  }
}